<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TSMTar_END_Date_must_chang_on_closure</fullName>
    <active>false</active>
    <errorConditionFormula>ISCHANGED( TSMTar_Status__c ) &amp;&amp; TEXT(TSMTar_Status__c) = &#39;Closed&#39; &amp;&amp; NOT(ISCHANGED( TSMTar_EndDate__c ))</errorConditionFormula>
    <errorDisplayField>TSMTar_EndDate__c</errorDisplayField>
    <errorMessage>Please update the end date of the plan.</errorMessage>
</ValidationRule>
