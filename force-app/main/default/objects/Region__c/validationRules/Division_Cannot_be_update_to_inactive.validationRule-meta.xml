<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Division_Cannot_be_update_to_inactive</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Region_NumberofChilds__c&gt;0,Text(Region_Status__c)=&#39;Inactive&#39;)</errorConditionFormula>
    <errorMessage>Division cannot be updated to Inactive ,as it contains the Active Region.</errorMessage>
</ValidationRule>
